                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT tRevision.Consecutivo, tRevision.Fecha, tInstitucion.CodigoAux, tInstitucion.Nombre, tInstitucion.DiaRuta, tRecepcion.Recepcion AS Recepción, tPersona.NombreCompleto, tRevision.Comentario, tEstado.Estado
