                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        tInstitucion.Codigo, tInstitucion.Circuito, tTipoInstitucion.NombreTipo, tInstitucion.Nombre, tInstitucion.CedulaJuridica, tInstitucion.DiaRuta, tInstitucion.CuentaLey, tInstitucion.Responsable, tInstitucion.Contacto, 
                         tPersona.NombreCompleto AS Contador, tInstitucion.Estado
FROM            tInstitucion INNER JOIN
                         tTipoInstitucion ON tInstitucion.Tipo = tTipoInstitucion.IdTipo INNER JOIN
                         tPersona ON tInstitucion.Contador = tPersona.Cedula
WHERE        (tInstitucion.Estado = 1)</CommandText>
